{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "linebreak-style": ["error", "windows"]
    },
    "ignorePatterns": ["/dist/"]
} 